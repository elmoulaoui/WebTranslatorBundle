{
    "name": "elmoulaoui/web-translator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle: Manage translation files with web pages",
    "keywords": ["Translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noureddine EL MOULAOUI",
            "email": "n.elmoulaoui@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "*",
        "symfony/yaml": "~2.1",
    },
    "target-dir": "Translator/WebTranslatorBundle",
    "repositories": [
    {
        "type": "git",
        "package": {
            "name": "elmoulaoui/WebTranslatorBundle",
            "version": "master",
            "source": {
                "url": "https://github.com/elmoulaoui/WebTranslatorBundle.git",
                "type": "https",
                "reference": "master"
            },
            "autoload": {
                "psr-0" : {
                    "Translator\\WebTranslatorBundle": ""
                }
            }
        }
    }
],
}